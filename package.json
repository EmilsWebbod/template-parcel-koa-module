{
  "name": "parcel-koa-module",
  "license": "MIT",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon",
    "web": "parcel watch index.html --dist-dir public",
    "parcel:build": "parcel build index.html --dist-dir public",
    "build": "yarn parcel:build && tsc --project tsconfig.prod.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "koa": "^2.13.4",
    "koa-cors": "^0.0.16",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@parcel/compressor-brotli": "^2.3.2",
    "@parcel/compressor-gzip": "^2.3.2",
    "@parcel/reporter-bundle-analyzer": "^2.3.2",
    "@types/koa-cors": "^0.0.2",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-test-renderer": "^17.0.1",
    "@vitest/ui": "^0.5.5",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "parcel": "^2.3.2",
    "prettier": "^2.5.1",
    "react-router": "^6.2.1",
    "react-test-renderer": "^17.0.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.4",
    "vitest": "^0.5.5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
